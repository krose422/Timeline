
<div class="all">

{{#each timelines}}

<div class="main">

  <div class="top">

    <div class="timestamp">
      <p class="date">{{date}}</p>
      <p class="time">{{times}}</p>
    </div>

    <div class="circleicon">
      <div class="circletwo">
        <div class="circleone">
          <i class="fa {{icon}}"></i>
        </div>
      </div>
    </div>

  </div> <!-- end top div -->


  <div class="maincontent">
    <div class="title">
      <div class="triangle"></div>
      <h3>{{title}}</h3>
    </div>

    <div class="content">
      <p>{{content}}</p>
    </div>

  </div> <!-- end maincontent div -->

</div> <!-- end main div -->

{{/each}}

</div> <!-- end all div -->


